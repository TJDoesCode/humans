{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": ["config:base", ":semanticCommitTypeAll(build)"],
	"labels": ["dependencies"],
	"packageRules": [
		{
			"updateTypes": ["minor"],
			"automerge": true
		},
		{
      "depTypeList": ["devDependencies"],
      "automerge": true
    }
	],
	"semanticCommits": true
}
